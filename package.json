{
  "private": true,
  "name": "memorall",
  "description": "Memorall - AI-powered memory and knowledge management browser extension built with Extension.js, TypeScript, and React.",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Zergling",
    "email": "zerglingno2@outlook.com"
  },
  "scripts": {
    "predev": "node public/tools/copy-transformers-assets.mjs",
    "dev": "concurrently \"npm run runner:dev\" \"npm run extension:dev\"",
    "start": "extension start",
    "runner:dev": "npx http-server runner -p 34115 --cors",
    "extension:dev": "extension dev",
    "prebuild": "node public/tools/copy-transformers-assets.mjs",
    "build": "extension build",
    "preview": "extension preview",
    "watch": "extension dev --watch",
    "build:prod": "NODE_ENV=production extension build",
    "build:runner:prepare": "rm -rf dist-gh && mkdir -p dist-gh && cp -R runner dist-gh/runner",
    "build:runner": "npm run build:runner:prepare && gh-pages -d dist-gh",
    "type-check": "tsc --noEmit",
    "clean": "extension clean",
    "lint": "extension lint",
    "format": "npx @biomejs/biome format --write ./src ./scripts ./public",
    "assets:transformers": "node public/tools/copy-transformers-assets.mjs"
  },
